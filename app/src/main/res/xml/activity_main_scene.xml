<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/commonActionBarEnd"
        motion:constraintSetStart="@id/commonActionBarStart"
        motion:duration="300">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/commonActionBarStart">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/commonActionBarEnd">
        <Constraint
            android:id="@+id/moveCopyActionModeBar"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="@id/divider"
            android:visibility="visible"
            motion:layout_constraintStart_toStartOf="parent"
            android:translationY="120dp" />
    </ConstraintSet>
</MotionScene>